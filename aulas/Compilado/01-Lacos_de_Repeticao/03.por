programa
{
	funcao inicio()
	{
		inteiro mes = 1
		inteiro dia = 1
		enquanto (mes <= 12)
		{
			dia = 1
			enquanto (dia <= 30)
			{
				escreva(dia + "º dia do " + mes + "º mes\n")
				dia++
			}
			escreva("\n")
			mes++
		}
	}
}
